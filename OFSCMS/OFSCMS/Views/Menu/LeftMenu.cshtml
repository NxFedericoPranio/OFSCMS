@model DataClasses.DataModel.Menu.OFSMenu 
@using OFSCore.Extension

<div class="module_menu">

                <h3>
                    @Model.Text</h3>
                <ul class="menu">
                    @foreach (DataClasses.DataModel.Menu.OFSMenu item in Model.MenuChildren.Children)
                    {
                        <li><a href="@Url.BuildMenuLink(item)" title="@item.Text"><span>@item.Text</span></a></li>
                    }
                </ul>

</div>